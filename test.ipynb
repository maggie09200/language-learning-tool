    "def get_answer_from_db(question,direction):\n",
    "    session = SessionLocal()\n",
    "    record = session.query(QARecord).filter_by(QARecord.question == question, QARecord.direction == QARecord.direction).first()\n",
    "    session.close()\n",
    "    if record:\n",
    "        return record.answer\n",
    "    return None"
   ]
